#ĞšĞ¾Ğ¼Ğ°Ğ½Ğ´Ğ° Ğ´Ğ»Ñ Ğ²Ğ¾Ğ¿Ñ€Ğ¾ÑĞ¾Ğ² Ğ´Ğ»Ñ Ğ±Ğ¾Ñ‚Ğ°
@client.command("Ğ²Ğ¾Ğ¿Ñ€Ğ¾Ñ")
def Ğ²Ğ¾Ğ¿Ñ€Ğ¾Ñ(data):
	lis = ['Ğ’Ğ¾Ğ·Ğ¼Ğ¾Ğ¶Ğ½Ğ¾', 'Ğ”Ğ°', 'ĞĞµÑ‚', 'ĞšĞ¾Ğ½ĞµÑ‡Ğ½Ğ¾', 'ĞĞ°Ğ²ĞµÑ€Ğ½Ğ¾Ğµ']
	msg = data.message + "null?"
	msg = data.message.split(" ")
	data.subClient.send_message(chatId=data.chatId, message=str(random.choice(lis)))

@client.command("Ğ´Ğ°Ğ¹ Ğ¿ĞµÑ‡ĞµĞ½ÑŒĞµ")
def sperm(data):
	msg = data.message + " null "
	msg = msg.split(" ")
	msg[2] = msg[1]
	msg[1] = msg[0]
	try:
		data.subClient.send_message(chatId=data.chatId, message=f"{msg[1]} Ã—Ğ´Ğ°Ğ» Ğ¿ĞµÑ‡ĞµĞ½ÑŒĞµÃ—")
	except:
		pass
