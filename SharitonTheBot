@client.command("google")
def google(data):
    msg = data.message.split(" ")
    msg = '+'.join(msg)
    data.subClient.send_message(chatId=data.chatId, message=f"https://www.google.com/search?q={msg}")

@client.command("Ğ¿Ğ¾Ğ³Ğ»Ğ°Ğ´Ğ¸Ñ‚ÑŒ Ğ¨Ğ°Ñ€Ğ¸Ñ‚Ğ¾Ğ½Ğ°")
def snus(data):
	msg = data.message + " null "
	msg = msg.split(" ")
	msg[1] = msg[0]
	try:
		data.subClient.send_message(chatId=data.chatId, message=f"{msg[1]} Ã—Ğ¼ÑƒÑ€Ğ»Ñ‹ĞºĞ°ĞµÑ‚Ã—")
	except:
		pass
