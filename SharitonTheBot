@client.on_member_join_chat()
def privet(data):
    data.subClient.send_message(data.chatId, f"Ğ”Ğ¾Ğ±Ñ€Ğ¾ ĞŸĞ¾Ğ¶Ğ°Ğ»Ğ¾Ğ²Ğ°Ñ‚ÑŒ Ğ² Ñ‡Ğ°Ñ‚ {data.author} Ñ Ğ±Ğ¾Ñ‚ Ğ¼Ğ¾Ğ¸ ĞºĞ¾Ğ¼Ğ°Ğ½Ğ´Ñ‹ !Ğ¨Ğ°Ñ€Ğ¸Ñ‚Ğ¾Ğ½)")


@client.on_member_leave_chat(["chatId"]) # the chatId is not necessary, put one if you want a specified chat only
def poka(data):
    data.subClient.send_message(data.chatId, f"Ğš ÑĞ¾Ğ¶Ğ°Ğ»ĞµĞ½Ğ¸Ñ {data.author} Ğ¿Ğ¾ĞºĞ¸Ğ½ÑƒĞ» Ñ‡Ğ°Ñ‚! ĞŸĞ¾Ğ¶ĞµĞ»Ğ°ĞµĞ¼ ĞµĞ¼Ñƒ ÑƒĞ´Ğ°Ñ‡Ğ¸!")

